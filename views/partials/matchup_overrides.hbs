                        <div class="mdc-line-ripple"></div>
                    </div>
                    <div id="override-adjacent-block" class="mdc-select mdc-select--box mdc-ripple-upgraded adjacent-block">
                        <select id="override-adjacent" class="mdc-select__native-control override-option adjacent-select battle-val">
                            <option value="0">0</option>
                            <option value="1">1</option>
                            <option value="2">2</option>
                            <option value="3">3</option>
                            <option value="4">4</option>
                        </select>
                        <label class="mdc-floating-label mdc-floating-label--float-above" for="adjacent-1">Adjacent Allies</label>
